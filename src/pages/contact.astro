---
import SeoHead from "../components/SeoHead.astro"
import JsonLd from "../components/JsonLd.astro"
import Base from "../layouts/Base.astro"
const canonical = new URL(Astro.url.pathname, Astro.site ?? "https://sandiegohandymanpro.co").toString()
const __TITLE = "Contact | San Diego Handyman Pros"
const __DESC = "Get a fast, fair quote for handyman services in San Diego County."
const __JSONLD = { "@context":"https://schema.org", "@type":"WebPage", "name": __TITLE, "url": canonical }
layout: ../layouts/Base.astro
---
<SeoHead title={__TITLE} description={__DESC} canonical={canonical} />
<JsonLd data={__JSONLD} />
<Base>
  <section class="mx-auto max-w-3xl px-4 py-12">
    <h1 class="text-3xl font-semibold">Contact</h1>
    <p class="mt-4 text-gray-700">Tell us about your project and weâ€™ll get back quickly.</p>
  </section>
</Base>
