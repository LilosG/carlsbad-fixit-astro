---
type AnyObj = Record<string, any>
interface Props { data?: AnyObj | AnyObj[] }
const { data } = Astro.props as Props
const blob = Array.isArray(data) ? data : data ? [data] : []
---
{blob.length > 0 && (
  <script type="application/ld+json" is:inline is:inline is:inline is:inline is:inline set:html={JSON.stringify(blob)} />
)}
